[32m+[m[32m        try {[m[41m[m
[32m+[m[32m            JSONObject jsonObject = new JSONObject();[m[41m[m
[32m+[m[32m            jsonObject.put("email", email.getText());[m[41m[m
[32m+[m[32m            jsonObject.put("password", securePassword);[m[41m[m
[32m+[m[32m            new URLSender().execute([m[41m[m
[32m+[m[32m                    SERVER_URL,[m[41m[m
[32m+[m[32m                    Action.LOGIN.value(),[m[41m[m
[32m+[m[32m                    jsonObject.toString()[m[41m[m
[32m+[m[32m            );[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[32m        catch(JSONException e){[m[41m[m
[32m+[m[32m            e.printStackTrace();[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        startActivity(intent);[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[32m}[m[41m[m
