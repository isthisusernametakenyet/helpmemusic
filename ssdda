[32m+[m				[32mlines.add(line);[m[41m[m
[32m+[m			[32m}[m[41m[m
[32m+[m			[32mdata = new DbAccessData([m[41m[m
[32m+[m					[32mlines.get(0),[m[41m[m
[32m+[m					[32mlines.get(1),[m[41m[m
[32m+[m					[32mlines.get(2),[m[41m[m
[32m+[m					[32mlines.get(3)[m[41m[m
[32m+[m			[32m);[m[41m[m
[32m+[m		[32m} catch (IOException ioe) {[m[41m[m
[32m+[m			[32mSystem.err.println("unable to read access data " + ioe.getMessage());[m[41m[m
[32m+[m			[32mSystem.exit(1);[m[41m[m
[32m+[m		[32m}[m[41m[m
[32m+[m		[32mreturn data;[m[41m[m
[32m+[m	[32m}[m[41m[m
[32m+[m[32m}[m[41m[m
