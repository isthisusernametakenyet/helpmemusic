[32m+[m[32m    private File createImageFile() throws IOException{[m[41m[m
[32m+[m[32m        PictureHash ph = new PictureHash("Marcus", "marcus@gmail.com");[m[41m[m
[32m+[m[32m        String fileName = "JPEG_" + ph.hash();[m[41m[m
[32m+[m[32m        File storageDir = getExternalFilesDir(Environment.DIRECTORY_PICTURES);[m[41m[m
[32m+[m[32m        File image = File.createTempFile(fileName, ".jpg", storageDir);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        mCurrentPhotoPath = image.getAbsolutePath();[m[41m[m
[32m+[m[32m        return image;[m[41m[m
[32m+[m[32m    }[m[41m[m
