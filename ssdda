[32m+[m[32m    public List<User> readFriends(String email) {[m[41m[m
[32m+[m[32m        final String SQL_SELECT_FRIENDS = "SELECT \"name\", \"email\", \"password\" "[m[41m[m
[32m+[m[32m                                        + "FROM friendship "[m[41m[m
[32m+[m[32m                                        + "JOIN usr "[m[41m[m
[32m+[m[32m                                        + "ON usr.email = friend_email "[m[41m[m
[32m+[m[32m                                        + "WHERE usr_email = '" + email + "'";[m[41m[m
[32m+[m[32m        DbUtil database = new DbUtil();[m[41m[m
[32m+[m[32m        Connection connection = database.connection();[m[41m[m
[32m+[m[32m        Statement statement = null;[m[41m[m
[32m+[m[32m        ResultSet rs = null;[m[41m[m
[32m+[m[32m        List<User> friends = new ArrayList<>();[m[41m[m
[32m+[m[32m        try {[m[41m[m
[32m+[m[32m            statement = connection.createStatement();[m[41m[m
[32m+[m[32m            rs = statement.executeQuery(SQL_SELECT_FRIENDS);[m[41m[m
[32m+[m[32m            while (rs.next()) {[m[41m[m
[32m+[m[32m                friends.add(new User([m[41m[m
[32m+[m[32m                            rs.getString("name"),[m[41m[m
[32m+[m[32m                            rs.getString("email"),[m[41m[m
[32m+[m[32m                            rs.getString("password"),[m[41m[m
[32m+[m[32m                            new ArrayList<User>()[m[41m[m
[32m+[m[32m                ));[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m        } catch (SQLException sqle) {[m[41m[m
[32m+[m[32m            System.out.println("unable to select from database "[m[41m [m
[32m+[m		[32m                  + sqle.getMessage());[m[41m[m
[32m+[m[32m            System.exit(1);[m[41m[m
