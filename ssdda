[32m+[m[32m    public void cameraClick(View view) {[m[41m[m
[32m+[m[32m        Intent intent = new Intent(MediaStore.ACTION_IMAGE_CAPTURE);[m[41m[m
[32m+[m[32m        if(intent.resolveActivity(getPackageManager()) != null) {[m[41m[m
[32m+[m[32m            Log.d(LOG_TAG, " Click camera");[m[41m[m
[32m+[m[32m                //intent.putExtra(MediaStore.EXTRA_OUTPUT, photoUri);[m[41m[m
[32m+[m[32m                startActivityForResult(intent, REQUEST_IMAGE_CAPTURE);[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[32m    }[m[41m[m
