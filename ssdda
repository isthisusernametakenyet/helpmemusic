[32m+[m[32m                JSONObject obj = new JSONObject();[m[41m[m
[32m+[m[32m                obj.put("imageFileName", ph.hash());[m[41m[m
[32m+[m[32m                obj.put("imageBitMapEncoded", imageString);[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m            catch (JSONException e){[m[41m[m
[32m+[m[32m                Log.e(LOG_TAG, " JsonException: " + e.getMessage());[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m            */[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m    private String bitmapToBase64(Bitmap bitmap) {[m[41m[m
[32m+[m[32m        ByteArrayOutputStream byteArrayOutputStream = new ByteArrayOutputStream();[m[41m[m
[32m+[m[32m        bitmap.compress(Bitmap.CompressFormat.PNG, 100, byteArrayOutputStream);[m[41m[m
[32m+[m[32m        byte[] byteArray = byteArrayOutputStream .toByteArray();[m[41m[m
[32m+[m[32m        return Base64.encodeToString(byteArray, Base64.DEFAULT);[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m    private Bitmap base64ToBitmap(String b64) {[m[41m[m
[32m+[m[32m        byte[] imageAsBytes = Base64.decode(b64.getBytes(), Base64.DEFAULT);[m[41m[m
[32m+[m[32m        return BitmapFactory.decodeByteArray(imageAsBytes, 0, imageAsBytes.length);[m[41m[m
[32m+[m[32m    }[m[41m[m
