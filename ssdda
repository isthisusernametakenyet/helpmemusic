[32m+[m[32m                char current = (char) inputStreamData;[m[41m[m
[32m+[m[32m                inputStreamData = inputStreamReader.read();[m[41m[m
[32m+[m[32m                data += current;[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m            System.out.println("TAG " + data);[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[32m        catch(Exception e){[m[41m[m
[32m+[m[32m            e.printStackTrace();[m[41m[m
[32m+[m[32m        }[m[41m[m
