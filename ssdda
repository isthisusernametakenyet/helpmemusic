[32m+[m[32mpublic class JSONParser {[m[41m[m
[32m+[m[41m[m
[32m+[m[32m    public JSONArray usersToJson(List<User> users) {[m[41m[m
[32m+[m[32m        JSONArray array = new JSONArray();[m[41m[m
[32m+[m[32m        for (User user : users) {[m[41m[m
[32m+[m[32m            JSONObject obj = new JSONObject();[m[41m[m
[32m+[m[32m            obj.put("name", user.name());[m[41m[m
[32m+[m[32m            obj.put("email", user.email());[m[41m[m
[32m+[m[32m            array.put(obj);[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[32m        return array;[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[41m             [m
[32m+[m[32m    public List<User> jsonToUsers(String json) {[m[41m                            [m
[32m+[m[32m        JSONArray jsonArray = null;[m[41m[m
[32m+[m[32m        try {[m[41m                  [m
[32m+[m[32m            JSONTokener jsonTokener = new JSONTokener(json);[m[41m[m
[32m+[m[32m            JSONObject jsonObject = new JSONObject(jsonTokener);[m[41m                 [m
[32m+[m[32m            jsonArray = jsonObject.getJSONArray("users");[m[41m                [m
[32m+[m[32m        } catch (JSONException je) {[m[41m                                       [m
[32m+[m[32m            System.err.println("unable to init json array: "[m[41m[m
[32m+[m[32m                    + je.getMessage());[m[41m[m
[32m+[m[32m            System.exit(1);[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[32m        List<User> users = new ArrayList<User>();[m[41m[m
[32m+[m[32m        for (int i = 0; i < jsonArray.length(); i++) {[m[41m[m
[32m+[m[32m            try {[m[41m[m
[32m+[m[32m                JSONObject jsonObj = jsonArray.getJSONObject(i);[m[41m[m
[32m+[m[32m                users.add(new User([m[41m[m
[32m+[m[32m                            jsonObj.getString("name"),[m[41m[m
[32m+[m[32m                            jsonObj.getString("email"),[m[41m[m
[32m+[m[32m                            jsonObj.getString("password"),[m[41m[m
[32m+[m[32m                            new ArrayList<User>()[m[41m[m
[32m+[m[32m                ));[m[41m            [m
[32m+[m[32m            } catch (JSONException je) {[m[41m[m
[32m+[m[32m                System.err.println("unable to parse: " + je.getMessage());[m[41m[m
[32m+[m[32m                System.exit(1);[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[32m        return users;[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m    public User jsonToUser(String json) {    // name not found at login[m[41m[m
[32m+[m[32m        User user = null;[m[41m[m
[32m+[m[32m        json = json.replace("PostData=", "");[m[41m[m
[32m+[m[32m        try {[m[41m[m
[32m+[m[32m            JSONTokener jsonTokener = new JSONTokener(json);[m[41m[m
[32m+[m[32m            JSONObject jo = new JSONObject(jsonTokener);[m[41m[m
[32m+[m[32m            user = new User([m[41m[m
[32m+[m[32m                    jo.getString("name"),[m[41m[m
[32m+[m[32m                    jo.getString("email"),[m[41m[m
[32m+[m[32m                    jo.getString("password"),[m[41m[m
[32m+[m[32m                    new ArrayList<User>());[m[41m[m
[32m+[m[32m        } catch (JSONException je) {[m[41m[m
[32m+[m[32m            System.err.println("unable to parse: " + je.getMessage());[m[41m[m
[32m+[m[32m            System.exit(1);[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[32m        return user;[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m    public String[] parseLogin(String json){[m[41m[m
[32m+[m[32m        String email = "";[m[41m[m
[32m+[m[32m        String password = "";[m[41m[m
[32m+[m[32m        try {[m[41m[m
[32m+[m[32m            JSONObject jsonObj = new JSONObject(json);[m[41m[m
[32m+[m[32m            email = jsonObj.getString("email");[m[41m[m
[32m+[m[32m            password = jsonObj.getString("password");[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[32m        catch (JSONException e) {[m[41m[m
[32m+[m[32m            System.err.println(e.getMessage());[m[41m   [m
[32m+[m[32m        }[m[41m[m
[32m+[m[32m        String[] loginValue = new String[2];[m[41m[m
[32m+[m[32m        loginValue[0] = email;[m[41m[m
[32m+[m[32m        loginValue[1] = password;[m[41m[m
[32m+[m[32m        return loginValue;[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[41m    [m
[32m+[m[32m    public String[] parseImageData(String json) {[m[41m[m
[32m+[m[32m        String imageName = "";[m[41m   [m
[32m+[m[32m        String imageData = "";[m[41m       [m
[32m+[m[32m        try{[m[41m[m
[32m+[m[32m            JSONObject jsonObj = new JSONObject(json);[m[41m[m
[32m+[m[32m            imageName = jsonObj.getString("imageFileName");[m[41m[m
[32m+[m[32m            imageData = jsonObj.getString("imageBitMapEncoded");[m[41m[m
[32m+[m[32m        } catch(JSONException e){[m[41m[m
[32m+[m[32m            System.out.println("faild to parse image data: " + e.getMessage());[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[32m        String[] imageValue = new String[2];[m[41m[m
[32m+[m[32m        imageValue[0] = imageName;[m[41m[m
[32m+[m[32m        imageValue[1] = imageData;[m[41m[m
[32m+[m[32m        return imageValue;[m[41m[m
[32m+[m[32m    }[m[41m[m
