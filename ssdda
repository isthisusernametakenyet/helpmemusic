[32m+[m[32m            try {[m[41m[m
[32m+[m[32m                statement.close();[m[41m[m
[32m+[m[32m                rs.close();[m[41m[m
[32m+[m[32m            } catch (SQLException sqle) {[m[41m[m
[32m+[m[32m                System.err.println("unable to close statement or resultset "[m[41m[m
[32m+[m[32m                        + sqle.getMessage());[m[41m[m
[32m+[m[32m                System.exit(1);[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m            database.disconnect();[m[41m[m
[32m+[m[32m        }[m[41m   [m
[32m+[m[32m        return friends;[m[41m[m
