[32m+[m[32m                                    + "FROM usr";[m[41m[m
[32m+[m[32m        List<User> users = new ArrayList<User>();[m[41m[m
[32m+[m[32m        DbUtil database = new DbUtil();[m[41m[m
[32m+[m[32m        Connection connection = database.connection();[m[41m[m
[32m+[m	[32mStatement stmt  = null;[m[41m[m
[32m+[m	[32mResultSet rs = null;[m[41m[m
[32m+[m	[32mtry {[m[41m[m
[32m+[m[32m            stmt = connection.createStatement();[m[41m[m
[32m+[m[32m            rs = stmt.executeQuery(SQL_SELECT_ALL);[m[41m[m
[32m+[m[32m            while (rs.next()) {[m[41m[m
[32m+[m[32m                users.add(new User([m[41m[m
[32m+[m[32m                            rs.getString("name"),[m[41m[m
[32m+[m[32m                            rs.getString("email"),[m[41m[m
[32m+[m[32m                            rs.getString("password"),[m[41m[m
[32m+[m[32m                            new ArrayList<User>()[m[41m[m
[32m+[m		[32m));[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m        } catch (SQLException sqle) {[m[41m[m
[32m+[m[32m            System.out.println("unable to select from database "[m[41m [m
[32m+[m		[32m                  + sqle.getMessage());[m[41m[m
[32m+[m[32m            System.exit(1);[m[41m[m
[32m+[m[32m        } finally {[m[41m[m
[32m+[m[32m            try {[m[41m[m
[32m+[m[32m                rs.close();[m[41m[m
[32m+[m[32m                stmt.close();[m[41m[m
[32m+[m[32m            } catch (SQLException sqle) {[m[41m[m
[32m+[m[32m                System.out.println("unable to close result set "[m[41m[m
[32m+[m				[32m  + sqle.getMessage());[m[41m[m
[32m+[m[32m                System.exit(1);[m[41m[m
