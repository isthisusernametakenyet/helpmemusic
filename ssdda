[32m+[m[41m[m
[32m+[m[32m# Determine the Java command to use to start the JVM.[m[41m[m
[32m+[m[32mif [ -n "$JAVA_HOME" ] ; then[m[41m[m
[32m+[m[32m    if [ -x "$JAVA_HOME/jre/sh/java" ] ; then[m[41m[m
[32m+[m[32m        # IBM's JDK on AIX uses strange locations for the executables[m[41m[m
[32m+[m[32m        JAVACMD="$JAVA_HOME/jre/sh/java"[m[41m[m
[32m+[m[32m    else[m[41m[m
[32m+[m[32m        JAVACMD="$JAVA_HOME/bin/java"[m[41m[m
[32m+[m[32m    fi[m[41m[m
[32m+[m[32m    if [ ! -x "$JAVACMD" ] ; then[m[41m[m
[32m+[m[32m        die "ERROR: JAVA_HOME is set to an invalid directory: $JAVA_HOME[m[41m[m
[32m+[m[41m[m
[32m+[m[32mPlease set the JAVA_HOME variable in your environment to match the[m[41m[m
[32m+[m[32mlocation of your Java installation."[m[41m[m
[32m+[m[32m    fi[m[41m[m
[32m+[m[32melse[m[41m[m
[32m+[m[32m    JAVACMD="java"[m[41m[m
[32m+[m[32m    which java >/dev/null 2>&1 || die "ERROR: JAVA_HOME is not set and no 'java' command could be found in your PATH.[m[41m[m
[32m+[m[41m[m
[32m+[m[32mPlease set the JAVA_HOME variable in your environment to match the[m[41m[m
[32m+[m[32mlocation of your Java installation."[m[41m[m
[32m+[m[32mfi[m[41m[m
[32m+[m[41m[m
[32m+[m[32m# Increase the maximum file descriptors if we can.[m[41m[m
[32m+[m[32mif [ "$cygwin" = "false" -a "$darwin" = "false" -a "$nonstop" = "false" ] ; then[m[41m[m
[32m+[m[32m    MAX_FD_LIMIT=`ulimit -H -n`[m[41m[m
[32m+[m[32m    if [ $? -eq 0 ] ; then[m[41m[m
[32m+[m[32m        if [ "$MAX_FD" = "maximum" -o "$MAX_FD" = "max" ] ; then[m[41m[m
[32m+[m[32m            MAX_FD="$MAX_FD_LIMIT"[m[41m[m
[32m+[m[32m        fi[m[41m[m
[32m+[m[32m        ulimit -n $MAX_FD[m[41m[m
[32m+[m[32m        if [ $? -ne 0 ] ; then[m[41m[m
[32m+[m[32m            warn "Could not set maximum file descriptor limit: $MAX_FD"[m[41m[m
[32m+[m[32m        fi[m[41m[m
[32m+[m[32m    else[m[41m[m
[32m+[m[32m        warn "Could not query maximum file descriptor limit: $MAX_FD_LIMIT"[m[41m[m
[32m+[m[32m    fi[m[41m[m
[32m+[m[32mfi[m[41m[m
[32m+[m[41m[m
[32m+[m[32m# For Darwin, add options to specify how the application appears in the dock[m[41m[m
[32m+[m[32mif $darwin; then[m[41m[m
[32m+[m[32m    GRADLE_OPTS="$GRADLE_OPTS \"-Xdock:name=$APP_NAME\" \"-Xdock:icon=$APP_HOME/media/gradle.icns\""[m[41m[m
[32m+[m[32mfi[m[41m[m
[32m+[m[41m[m
[32m+[m[32m# For Cygwin, switch paths to Windows format before running java[m[41m[m
[32m+[m[32mif $cygwin ; then[m[41m[m
[32m+[m[32m    APP_HOME=`cygpath --path --mixed "$APP_HOME"`[m[41m[m
[32m+[m[32m    CLASSPATH=`cygpath --path --mixed "$CLASSPATH"`[m[41m[m
[32m+[m[32m    JAVACMD=`cygpath --unix "$JAVACMD"`[m[41m[m
[32m+[m[41m[m
[32m+[m[32m    # We build the pattern for arguments to be converted via cygpath[m[41m[m
[32m+[m[32m    ROOTDIRSRAW=`find -L / -maxdepth 1 -mindepth 1 -type d 2>/dev/null`[m[41m[m
[32m+[m[32m    SEP=""[m[41m[m
[32m+[m[32m    for dir in $ROOTDIRSRAW ; do[m[41m[m
[32m+[m[32m        ROOTDIRS="$ROOTDIRS$SEP$dir"[m[41m[m
[32m+[m[32m        SEP="|"[m[41m[m
[32m+[m[32m    done[m[41m[m
[32m+[m[32m    OURCYGPATTERN="(^($ROOTDIRS))"[m[41m[m
[32m+[m[32m    # Add a user-defined pattern to the cygpath arguments[m[41m[m
[32m+[m[32m    if [ "$GRADLE_CYGPATTERN" != "" ] ; then[m[41m[m
[32m+[m[32m        OURCYGPATTERN="$OURCYGPATTERN|($GRADLE_CYGPATTERN)"[m[41m[m
[32m+[m[32m    fi[m[41m[m
[32m+[m[32m    # Now convert the arguments - kludge to limit ourselves to /bin/sh[m[41m[m
[32m+[m[32m    i=0[m[41m[m
[32m+[m[32m    for arg in "$@" ; do[m[41m[m
[32m+[m[32m        CHECK=`echo "$arg"|egrep -c "$OURCYGPATTERN" -`[m[41m[m
[32m+[m[32m        CHECK2=`echo "$arg"|egrep -c "^-"`                                 ### Determine if an option[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        if [ $CHECK -ne 0 ] && [ $CHECK2 -eq 0 ] ; then                    ### Added a condition[m[41m[m
[32m+[m[32m            eval `echo args$i`=`cygpath --path --ignore --mixed "$arg"`[m[41m[m
[32m+[m[32m        else[m[41m[m
[32m+[m[32m            eval `echo args$i`="\"$arg\""[m[41m[m
[32m+[m[32m        fi[m[41m[m
[32m+[m[32m        i=$((i+1))[m[41m[m
[32m+[m[32m    done[m[41m[m
[32m+[m[32m    case $i in[m[41m[m
[32m+[m[32m        (0) set -- ;;[m[41m[m
[32m+[m[32m        (1) set -- "$args0" ;;[m[41m[m
[32m+[m[32m        (2) set -- "$args0" "$args1" ;;[m[41m[m
[32m+[m[32m        (3) set -- "$args0" "$args1" "$args2" ;;[m[41m[m
[32m+[m[32m        (4) set -- "$args0" "$args1" "$args2" "$args3" ;;[m[41m[m
[32m+[m[32m        (5) set -- "$args0" "$args1" "$args2" "$args3" "$args4" ;;[m[41m[m
[32m+[m[32m        (6) set -- "$args0" "$args1" "$args2" "$args3" "$args4" "$args5" ;;[m[41m[m
[32m+[m[32m        (7) set -- "$args0" "$args1" "$args2" "$args3" "$args4" "$args5" "$args6" ;;[m[41m[m
[32m+[m[32m        (8) set -- "$args0" "$args1" "$args2" "$args3" "$args4" "$args5" "$args6" "$args7" ;;[m[41m[m
[32m+[m[32m        (9) set -- "$args0" "$args1" "$args2" "$args3" "$args4" "$args5" "$args6" "$args7" "$args8" ;;[m[41m[m
[32m+[m[32m    esac[m[41m[m
[32m+[m[32mfi[m[41m[m
[32m+[m[41m[m
[32m+[m[32m# Escape application args[m[41m[m
[32m+[m[32msave () {[m[41m[m
[32m+[m[32m    for i do printf %s\\n "$i" | sed "s/'/'\\\\''/g;1s/^/'/;\$s/\$/' \\\\/" ; done[m[41m[m
[32m+[m[32m    echo " "[m[41m[m
[32m+[m[32m}[m[41m[m
[32m+[m[32mAPP_ARGS=$(save "$@")[m[41m[m
[32m+[m[41m[m
[32m+[m[32m# Collect all arguments for the java command, following the shell quoting and substitution rules[m[41m[m
[32m+[m[32meval set -- $DEFAULT_JVM_OPTS $JAVA_OPTS $GRADLE_OPTS "\"-Dorg.gradle.appname=$APP_BASE_NAME\"" -classpath "\"$CLASSPATH\"" org.gradle.wrapper.GradleWrapperMain "$APP_ARGS"[m[41m[m
[32m+[m[41m[m
[32m+[m[32m# by default we should be in the correct project dir, but when run from Finder on Mac, the cwd is wrong[m[41m[m
[32m+[m[32mif [ "$(uname)" = "Darwin" ] && [ "$HOME" = "$PWD" ]; then[m[41m[m
[32m+[m[32m  cd "$(dirname "$0")"[m[41m[m
[32m+[m[32mfi[m[41m[m
[32m+[m[41m[m
[32m+[m[32mexec "$JAVACMD" "$@"[m[41m[m
[1mdiff --git a/client/gradlew.bat b/client/gradlew.bat[m
[1mindex f955316..e95643d 100644[m
