[32m+[m[32m    protected void onActivityResult(int requestCode, int resultCode, Intent data) {[m[41m[m
[32m+[m[32m        if(requestCode == REQUEST_IMAGE_CAPTURE && resultCode == RESULT_OK){[m[41m[m
[32m+[m[32m            Log.d(LOG_TAG, " data.getExtra() " + data.getExtras().toString());[m[41m[m
[32m+[m[32m            Bundle extras = data.getExtras();[m[41m[m
[32m+[m[32m            Bitmap imageBitmap = (Bitmap) extras.get("data");[m[41m[m
[32m+[m[32m            String imageString = bitmapToBase64(imageBitmap);[m[41m[m
[32m+[m[32m            Log.d(LOG_TAG, " Base64 encode " + imageString);[m[41m[m
[32m+[m[32m            mImageView.setImageBitmap(base64ToBitmap(imageString));[m[41m[m
[32m+[m[32m            Log.d(LOG_TAG, " Decode base64 to bitmap");[m[41m[m
[32m+[m[32m            /*[m[41m[m
