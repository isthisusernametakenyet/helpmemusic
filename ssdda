[32m+[m[32m    public User readUser(String email, String password) {[m[41m[m
[32m+[m[32m        final String SQL_SELECT = "SELECT * "[m[41m[m
[32m+[m[32m                                + "FROM usr "[m[41m[m
[32m+[m[32m                                + "WHERE \"email\" = '"[m[41m [m
[32m+[m[32m                                + email + "' AND \"password\" = '"[m[41m [m
[32m+[m[32m                                + password + "'";[m[41m[m
[32m+[m	[32mUser user = null;[m[41m[m
[32m+[m[32m        DbUtil database = new DbUtil();[m[41m[m
[32m+[m	[32mConnection connection = database.connection();[m[41m[m
[32m+[m	[32mStatement stmt  = null;[m[41m[m
[32m+[m	[32mResultSet rs = null;[m[41m[m
[32m+[m	[32mtry {[m[41m[m
[32m+[m[32m            stmt = connection.createStatement();[m[41m[m
[32m+[m[32m            rs = stmt.executeQuery(SQL_SELECT);[m[41m[m
[32m+[m[32m            if (rs.next()) {[m[41m[m
[32m+[m[32m                user = new User([m[41m[m
[32m+[m		[32m                rs.getString("name"),[m[41m[m
[32m+[m				[32mrs.getString("email"),[m[41m[m
[32m+[m				[32mrs.getString("password"),[m[41m[m
[32m+[m				[32mnew ArrayList<User>()[m[41m[m
[32m+[m		[32m);[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m	[32m} catch (SQLException sqle) {[m[41m[m
[32m+[m[32m            System.err.println("unable to select from database " + sqle.getMessage());[m[41m[m
[32m+[m[32m            System.exit(1);[m[41m[m
[32m+[m	[32m} finally {[m[41m[m
[32m+[m[32m            try {[m[41m[m
[32m+[m[32m                rs.close();[m[41m[m
[32m+[m		[32mstmt.close();[m[41m[m
[32m+[m[32m            } catch (SQLException sqle) {[m[41m[m
[32m+[m[32m                System.out.println("unable to close result set or statement "[m[41m [m
[32m+[m					[32m+ sqle.getMessage());[m[41m[m
[32m+[m[32m                System.exit(1);[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m            database.disconnect();[m[41m[m
[32m+[m	[32m}[m[41m[m
[32m+[m	[32mreturn user;[m[41m[m
[32m+[m[32m    }[m[41m[m
