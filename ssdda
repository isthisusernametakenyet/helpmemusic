[32m+[m[32mpublic class PasswordHashing {[m[41m[m
[32m+[m[41m[m
[32m+[m[32m    public String getSHA256SecurePassword(String passwordToHash){[m[41m[m
[32m+[m[32m        String generatedPassword = null;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        try{[m[41m[m
[32m+[m[32m            MessageDigest md = MessageDigest.getInstance("SHA-256");[m[41m[m
[32m+[m[41m[m
