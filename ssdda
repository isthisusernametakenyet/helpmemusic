[32m+[m[32mpublic class URLSender extends AsyncTask<String, Void, String> {[m[41m[m
[32m+[m[41m[m
[32m+[m[32m    public String doInBackground(String... params){[m[41m[m
[32m+[m[32m        String data = "";[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        HttpURLConnection connection = null;[m[41m[m
[32m+[m[32m        try{[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            connection = (HttpURLConnection) new URL(params[0]).openConnection();[m[41m[m
[32m+[m[32m            connection.setRequestMethod("POST");[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            connection.setDoOutput(true);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            DataOutputStream wr = new DataOutputStream(connection.getOutputStream());[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            wr.writeBytes(params[1] + params[2]);[m[41m[m
[32m+[m[32m            wr.flush();[m[41m[m
[32m+[m[32m            wr.close();[m[41m[m
